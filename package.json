{
	"name": "@alwalxed/yuta",
	"version": "1.0.1",
	"description": "Yuta simplifies domain enumeration by handling domain discovery, DNS probing, HTTP probing, and result filtering in one go.",
	"keywords": [
		"domain-enumeration",
		"dns-probing",
		"http-probing",
		"amass",
		"dnsx",
		"httpx",
		"docker",
		"subdomain-discovery",
		"web-security",
		"command-line-tool",
		"open-source",
		"automated-probing",
		"cli",
		"cli-tool",
		"hacking",
		"security",
		"subdomain-enumeration",
		"pentesting",
		"pentest",
		"reconnaissance",
		"recon",
		"domain-discovery",
		"domain-probing"
	],
	"author": "alwalxed",
	"repository": {
		"type": "git",
		"url": "https://github.com/alwalxed/yuta"
	},
	"bugs": {
		"url": "https://github.com/alwalxed/yuta/issues"
	},
	"private": false,
	"license": "MIT",
	"bin": {
		"yuta": "dist/cli.js"
	},
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"start": "node dist/cli.js",
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "npx prettier -w . && prettier --check . && xo && ava"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"dependencies": {
		"ansi-regex": "^6.0.1",
		"dockerode": "^4.0.2",
		"ink": "^4.1.0",
		"ink-big-text": "^2.0.0",
		"ink-gradient": "^3.0.0",
		"meow": "^11.0.0",
		"nanoid": "^5.0.7",
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/dockerode": "^3.3.30",
		"@types/node": "^20.14.11",
		"@types/react": "^18.0.32",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-unicorn": "^54.0.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^2.8.7",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3",
		"xo": "^0.53.1"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off",
			"unicorn/expiring-todo-comments": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config"
}
